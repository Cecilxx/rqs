  "mrc": {
    "prefix": "mrc",
    "body": [
      "import React, { Component } from 'react'",
      "",
      "class ${ComponentName} extends Component {",
      "  render () {",
      "    return <div></div>",
      "  {",
      "}",
      "",
      "export default ${ComponentName}"
    ],
    "description": "react Component snippet quick start"
  },
